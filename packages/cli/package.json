{
  "private": true,
  "name": "@straw-hat/cli",
  "version": "0.10.3",
  "description": "Straw Hat Team CLI",
  "repository": {
    "type": "git",
    "url": "https://github.com/straw-hat-team/javascript.git",
    "directory": "packages/tooling/cli"
  },
  "keywords": [
    "oclif",
    "cli",
    "straw-hat-team"
  ],
  "oclif": {
    "commands": "./dist/commands",
    "bin": "shc",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-plugins"
    ],
    "topics": {
      "gen": {
        "description": "multiple generators"
      },
      "fe": {
        "description": "frontend commands"
      }
    }
  },
  "scripts": {
    "cli": "bin/run",
    "prepack": "yarn build && yarn oclif:manifest",
    "postpack": "rm -f oclif.manifest.json",
    "prepare": "yarn build",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.prod.json",
    "start": "tsc -w -p tsconfig.json",
    "oclif:manifest": "oclif-dev manifest dist",
    "oclif:readme": "oclif-dev readme --multi  --dir=docs/commands"
  },
  "engines": {
    "node": ">=12.18.0"
  },
  "bin": {
    "shc": "bin/run"
  },
  "type": "commonjs",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "bin",
    "dist",
    "docs",
    "oclif.manifest.json",
    "README.md"
  ],
  "dependencies": {
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/errors": "1.3.4",
    "@oclif/plugin-autocomplete": "0.3.0",
    "@oclif/plugin-help": "3.2.1",
    "@oclif/plugin-plugins": "1.9.5",
    "@straw-hat/cli-core": "0.10.3",
    "@straw-hat/jest-config-chain": "1.0.2",
    "debug": "4.3.1",
    "del": "6.0.0",
    "enquirer": "2.3.6",
    "envinfo": "7.7.3",
    "jest-watch-typeahead": "0.6.1",
    "ts-jest": "26.4.4",
    "jest": "26.6.3",
    "jest-cli": "26.6.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.26.0"
  }
}
