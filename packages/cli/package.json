{
  "private": true,
  "name": "@straw-hat/cli",
  "version": "0.10.3",
  "description": "Straw Hat Team CLI",
  "repository": {
    "type": "git",
    "url": "https://github.com/straw-hat-team/javascript.git",
    "directory": "packages/tooling/cli"
  },
  "keywords": [
    "oclif",
    "cli",
    "straw-hat-team"
  ],
  "oclif": {
    "commands": "./dist/commands",
    "bin": "shc",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-plugins"
    ]
  },
  "scripts": {
    "cli": "bin/run",
    "prepack": "yarn build && yarn oclif:manifest",
    "postpack": "rm -f oclif.manifest.json",
    "prepare": "yarn build",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.prod.json",
    "start": "tsc -w -p tsconfig.json",
    "oclif:manifest": "oclif-dev manifest dist",
    "oclif:readme": "oclif-dev readme --multi  --dir=docs/commands"
  },
  "engines": {
    "node": ">=12.18.0"
  },
  "bin": {
    "shc": "bin/run"
  },
  "type": "commonjs",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "bin",
    "dist",
    "docs",
    "oclif.manifest.json",
    "README.md"
  ],
  "dependencies": {
    "@straw-hat/cli-core": "0.10.3",
    "@oclif/config": "1.13.3",
    "@oclif/errors": "1.2.2",
    "@oclif/plugin-autocomplete": "0.1.4",
    "@oclif/plugin-help": "2.2.1",
    "@oclif/plugin-plugins": "1.7.8",
    "chalk": "3.0.0",
    "ci-info": "2.0.0",
    "debug": "4.1.1",
    "del": "5.1.0",
    "enquirer": "2.3.2",
    "envinfo": "7.5.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.2"
  }
}
